# ============================================
# Docker Environment Variables
# ============================================
# Copie para .env antes de executar docker-compose up

# ============================================
# DATABASE (PostgreSQL)
# ============================================
POSTGRES_PASSWORD=your_strong_password_here_change_me

# ============================================
# NEXTAUTH
# ============================================
NEXTAUTH_URL=http://localhost:3001
NEXTAUTH_SECRET=your_secret_generated_with_openssl_rand_base64_32

# ============================================
# REDIS
# ============================================
REDIS_PASSWORD=your_redis_password_here_change_me

# ============================================
# MINIO (S3-compatible storage)
# ============================================
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=your_minio_password_here_change_me

# ============================================
# OAUTH PROVIDERS (opcional)
# ============================================
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

GITHUB_ID=your_github_oauth_app_id_here
GITHUB_SECRET=your_github_oauth_secret_here
